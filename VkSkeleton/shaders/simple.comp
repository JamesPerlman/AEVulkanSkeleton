#version 450

layout (set = 0, binding = 0) readonly buffer InputBuffer {
    uint data[];
} inputBuffer;

layout (set = 0, binding = 1) buffer OutputBuffer {
    uint data[];
} outputBuffer;

void main()
{
    uint gID = 32 * gl_GlobalInvocationID.x + gl_GlobalInvocationID.y;
    
    outputBuffer.data[gID] = inputBuffer.data[gID];
}
